spring.application.name=ufs.saude
server.port=8080

# URL de conexão com o banco de dados. O padrão para o Docker é este.
spring.datasource.url=jdbc:postgresql://localhost:5432/saude

# Credenciais do banco de dados. O padrão para o Docker é 'postgres'.
spring.datasource.username=postgres
spring.datasource.password=postgres

spring.jpa.hibernate.ddl-auto=update
spring.jpa.show-sql=true
spring.jpa.properties.hibernate.format_sql=true

# URL de conexão com o MongoDB. O padrão para o Docker é este.
spring.data.mongodb.uri=mongodb://localhost:27017/saude

springdoc.api-docs.path=/api-docs
